{
   "env": {
      "browser": true,
      "es6": true
   },
   "extends": ["airbnb", "prettier"],

   "plugins": ["prettier"],
   "rules": {
      "no-unused-vars": "off",
      "no-console": "off",
      "no-shadow": "error",
      "no-alert": "off",
      "no-plusplus": "off",
      "one-var": "off",
      "radix": "off",
      "consistent-return": "off",
      "no-restricted-globals": "off"
   }
}
